clc;
close all;
clear all;
x = input('enter the value:');
n = length(x);
disp(n);
len = (2^n)-1;
disp(len);
ff=[];
ff(1)= input('1: ');
ff(2)= input('2: ');
ff(3)= input('3: ');
ff(4)= input('4: ');
for i=1:len;
    temp = ff(1);
    ff(1)=xor(ff(4),ff(1));
    ff(4)= ff(3);
    ff(3)= ff(2);
    ff(2)= temp;
    disp(ff(4));
End
